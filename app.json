{
  "expo": {
    "name": "meuAppSmooth",
    "slug": "meuAppSmooth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-removebg-preview.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain ",
      "backgroundColor": "#F8F9FA"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
